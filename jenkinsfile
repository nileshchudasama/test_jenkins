pipeline{
	agent any
		stages
		{
		 parallel{
				stage('Build') {
						steps{
							echo "Built successfully"
						}
				}
				stage('Test') {
					steps {
						echo "Test Successfully"
					}
				}
				stage('Deploy')
				{
					steps {
						echo "deploy Successfully"
					}
				}
			}
		}
}
